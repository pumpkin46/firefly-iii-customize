<div class="{{ classes }}" id="{{ name }}_holder">
    <label for="{{ options.id }}" class="col-sm-4 control-label">{{ label }}</label>

    <div class="col-sm-8">
        {{ Form.select(name, list, selected , options ) }}
        {% include 'v1.form.help' %}
        {% include 'v1.form.feedback' %}

    </div>
</div>
